<configuration>
	<system.web>
		<!-- Uncomment if using IIS 7 Classic Mode or IIS 6 -->
		<!--
		<httpHandlers>
			<add verb="GET" path="*.jsx" type="React.Web.JsxHandlerFactory, React.Web" />
		</httpHandlers>
		-->
	</system.web>

	<system.webServer>
		<handlers>
